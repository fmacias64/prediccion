{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug FastAPI",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "main:app",
          "--reload",
          "--host",
          "127.0.0.1",
          "--port",
          "8000"
